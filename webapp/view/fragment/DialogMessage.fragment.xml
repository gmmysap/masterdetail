<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
>
    <Dialog>
        <content>
            <List
                headerText="Fehlers"
                items="{path: 'message>/MessagesList'}"
            >
                <items>
                    <ObjectListItem
                        title="{message>datatime}"
                        number="{message>messtxt}"
                    />
                </items>
            </List>
        </content>

        <buttons>
            <Button
                text="Clear"
                press="onDialogMessageClear"
            />
            <Button
                text="Cacel"
                type="Transparent"
                press="onDialogMessageCancel"
            />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
